program -> test_program, test_main;						// создать (объявить) подпрограммы


test_program := (set -> set_1), 			        	// заголовок подпрограммы test_program с параметрами set_1
    set -> set_3 := {set_2, {}};

    set_1 := [set_2, terminal -> <alias>] as [<alias>];	// создать множество терминалов множества set_2 как значения переменной set_1
    				     								// предыдущее значение из памяти не удалить
    set_2 := [set_1, set -> <alias>] as [<alias>];		// создать множество множеств, принадлежащих множеству set_1, как значения переменной set_2
    													// предыдущее значение из памяти не удалить
    remove -> [set_1, terminal -> <alias>];				// удалить все терминалы из множества set_1;
    remove -> [set_1, set -> <alias>];

    return -> set_1;

test_main := (), 										// заголовок подпрограммы test_main
    set -> set_1, set_2; 								// создать два множества как значения переменных set_1, set_2;
    terminal -> el_1, el_2, el3; 						// создать три терминальные сущности, не являющиеся множествами
    set_2 -> el_1, el_2, el3; 							// добавить терминалы во множество set_2

    set_1 := test_program -> set_1, set_2;				// вызвать подпрограмму test_program и передать параметры set_1, set_2

    return -> {};
